{
  "name": "laravel-doctrine/extensions",
  "description": "Doctrine extensions for Doctrine 2 and Laravel",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "doctrine",
    "gedmo",
    "beberlei",
    "laravel",
    "orm",
    "data mapper",
    "database"
  ],
  "authors": [
    {
      "name": "Patrick Brouwers",
      "email": "patrick@maatwebsite.nl"
    }
  ],
  "require": {
    "php": "^7.2.5|^8.0",
    "illuminate/auth": "^6.0|^7.0|^8.0",
    "illuminate/config": "^6.0|^7.0|^8.0",
    "illuminate/contracts": "^6.0|^7.0|^8.0",
    "illuminate/support": "^6.0|^7.0|^8.0",
    "illuminate/http": "^6.0|^7.0|^8.0",
    "laravel-doctrine/orm": "^1"
  },
  "suggest": {
    "gedmo/doctrine-extensions": "Behavioral Doctrine2 extensions",
    "beberlei/doctrineextensions": "Query/Type Doctrine2 extensions (^1.0)"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0 | ^8.3 | ^9.3",
    "mockery/mockery": "^1.3.1",
    "gedmo/doctrine-extensions": "^2.4|^3.0",
    "beberlei/doctrineextensions": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "LaravelDoctrine\\Extensions\\": "src/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests"
    ]
  }
}
